{
  "name": "hyperliquid-ts",
  "version": "1.0.0",
  "description": "\uD83E\uDE96 Unofficial TypeScript SDK for Hyperliquid",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist && bun run tsc --emitDeclarationOnly --outDir dist",
    "test": "bun test",
    "lint": "bun eslint . --ext .ts",
    "format": "bun x prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "bun run build",
    "publish": "npm publish"
  },
  "keywords": [
    "hyperliquid",
    "api",
    "sdk",
    "rest",
    "websocket",
    "quant",
    "trading",
    "crypto",
    "blockchain",
    "dex",
    "bun"
  ],
  "author": "majinbot",
  "license": "MIT",
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0-beta2",
    "axios": "^1.7.4",
    "ethers": "^6.13.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "bun-types": "latest",
    "eslint": "^8.57.0",
    "prettier": "^2.8.8",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "bun": ">=1.1.2x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/majinbot/hl-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/majinbot/hl-sdk/issues"
  },
  "homepage": "https://github.com/majinbot/hl-sdk#readme"
}